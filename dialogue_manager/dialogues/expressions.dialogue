# 葛城
# 默认
do Character("葛城").SetParts("Body:手放下,Eyebrows:默认,Eyes:默认,Mouth:闭口微笑")

# 坏笑
do Character("葛城").SetParts("Body:手放下,Eyebrows:下挑,Eyes:微眯,Mouth:露齿笑")

# 开玩笑
do Character("葛城").SetParts("Body:手放下,Eyebrows:上挑,Eyes:微眯,Mouth:说话")

# 闭眼大笑
do Character("葛城").SetParts("Body:手放下,Eyebrows:上挑,Eyes:眯眼,Mouth:说话")
